@model IEnumerable<SampleViewModel>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>File Name</th>
        <th>Image Name</th>
        <th>Image</th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item.File.Name
        </td>
        <td>
            @item.Image.Name
        </td>
        <td>
            <img src="@item.Image.Content" />            
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id })
        </td>
    </tr>
}

</table>
